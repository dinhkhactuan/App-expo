{
  "expo": {
    "name": "app-1",
    "slug": "app-1",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/google_icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/google_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/google_icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.dinhtuan.app1"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "1ed91cef-99db-4268-bab5-b81312ed7b43"
      }
    }
  }
}
